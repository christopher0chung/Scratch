			float3 ase_vertex3Pos = mul( unity_WorldToObject, float4( i.worldPos , 1 ) );
			float4 appendResult10 = (float4(0.0 , ( _Time.y * -5 ) , 0.0 , 0.0));
			float simplePerlin3D2 = snoise( (ase_vertex3Pos*_DetailScale + appendResult10.xyz) );
			float temp_output_20_0 = (0.0 + (simplePerlin3D2 - -1.0) * (1.0 - 0.0) / (1.0 - -1.0));
			float4 lerpResult15 = lerp( _Color0 , _Color1 , temp_output_20_0);
			o.Emission = lerpResult15.rgb;
			float3 ase_worldPos = i.worldPos;
			float3 ase_worldViewDir = normalize( UnityWorldSpaceViewDir( ase_worldPos ) );
			float3 ase_worldNormal = i.worldNormal;
			float fresnelNdotV22 = dot( ase_worldNormal, ase_worldViewDir );
			float fresnelNode22 = ( 0.0 + 1.0 * pow( 1.0 - fresnelNdotV22, 1.43 ) );
			float temp_output_25_0 = (fresnelNode22*-1.0 + 1.0);
			o.Alpha = ( temp_output_20_0 * temp_output_25_0 * temp_output_25_0 * temp_output_25_0 * temp_output_25_0 );
		}

		ENDCG
